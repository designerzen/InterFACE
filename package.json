{
	"name": "interface.place",
	"description": "Instant Musical Superpowers! Body controllable accessible musical synthesizer",
	"version": "0.20.0",
	"time": 1694451105599,
	"author": "designerzen <designerzen@gmail.com>",
	"license": "GPL-2.0-only",
	"repository": "https://github.com/designerzen/InterFACE.git",
	"bugs": "https://github.com/designerzen/InterFACE/issues",
	"homepage": "https://interface.place",
	"keywords": [
		"music",
		"accessibility",
		"machine learning",
		"ai",
		"xr",
		"ar"
	],
	"scripts": {
		"test": "jest",
		"push:": "git push origin --tags && git push origin",
		"hardreset": "node actions/reset.mjs --total && npm ci",
		"reset": "node actions/reset.mjs",
		"freshstart": "npm run reset && npm run start",
		"start": "parcel serve source/*.pug -p 909",
		"index": "parcel serve source/index.pug -p 909",
		"wam": "parcel serve source/wam.pug",
		"plop": "parcel serve source/interface.pug",
		"killport": "npx kill-port 909",
		"upgrader": "yarn upgrade-interactive",
		"changes": "node actions/changelog.mjs",
		"clean": "node actions/clean.mjs",
		"copy": "node actions/copy.mjs",
		"clear": "npm run clean && npm run copy",
		"deploy": "node actions/deploy.mjs",
		"doc": "jsdoc -d docs --configure .jsdoc.json source/interface.js",
		"build": "npm run build:index",
		"build:wam": "parcel build --no-content-hash --public-url ./ source/wam.pug",
		"build:debug": "parcel build --no-minify --no-scope-hoist ---no-content-hash --public-url ./  source/*.pug",
		"build:index": "parcel build --no-source-maps --no-scope-hoist --no-content-hash --public-url ./ source/index.pug",
		"build:workers": "parcel build --no-scope-hoist --no-content-hash --public-url ./ source/*/**-worker.js",
		"run:electron": "electron . --enable-logging",
		"test:electron": "electron ./main.cjs --enable-logging",
		"build:electron": "electron .",
		"package:electron": "electron-packager . photosynth-interface --overwrite --out=releases --icon=./spirce/assets/icons/icon.ico",
		"open:electron": "npx cap open @capacitor-community/electron",
		"build:android": "parcel build --public-url ./ --dist-dir ./dist --no-source-maps source/app.pug",
		"open:android": "npx cap open android",
		"run:android": "npx cap run android",
		"android": "npm run clear && npm run build:android && node actions/app.mjs && npm run open:android",
		"build:help": "parcel build --public-url ./ source/help.pug",
		"build:roadmap": "parcel build --public-url ./ source/roadmap.pug",
		"app": "parcel build --public-url ./ --dist-dir ./app --no-source-maps source/app.pug",
		"app:build": "parcel build --public-url ./ --dist-dir ./app --no-source-maps source/app.pug",
		"copyrights:list": "NODE_ENV=production yarn licenses list && node license.mjs",
		"license": "yarn licenses generate-disclaimer > LICENSES.md && yarn licenses generate-disclaimer > source/licenses.md",
		"license:build": "parcel build --public-url ./ source/licenses.pug",
		"patch": "changelog -p && npm version patch --force && node actions/revision.mjs",
		"minor": "changelog -m && npm version minor --force && node actions/revision.mjs",
		"major": "changelog -M && npm version major --force && node actions/revision.mjs",
		"package": "npm run minor && npm run clear && npm run build",
		"analyse": "parcel build source/index.pug --profile --detailed-report",
		"publish:www": "npm run clear && npm run build && npm run deploy",
		"publish:cloudflare": "npm run clean && npm run build && npm run deploy",
		"release:patch": "npm run patch && npm run publish:www",
		"release:minor": "npm run minor && npm run publish:www",
		"release:major": "npm run major && npm run publish:www",
		"release:app": "npm run patch && npm run push",
		"status": "electron-packager --version"
	},
	"staticFiles": {
		"staticFiles": [
			{
				"staticPath": "static"
			},
			{
				"staticPath": "./node_modules/@tensorflow/tfjs-backend-wasm/dist",
				"staticOutPath": "tf/"
			},
			{
				"staticPath": "./node_modules/@mediapipe/face_mesh",
				"staticOutPath": "@mediapipe/"
			},
			{
				"staticPath": "./node_modules/@mediapipe/tasks-vision/wwasm",
				"staticOutPath": "@mediapipe/"
			}
		]
	},
	"dependencies": {
		"@analytics/google-analytics": "^1.0.7",
		"@capacitor/electron": "^2.5.0",
		"@lookingglass/webxr": "^0.4.0",
		"@mediapipe/face_detection": "^0.4.1646425229",
		"@mediapipe/face_mesh": "^0.4.1633559619",
		"@mediapipe/pose": "^0.5.1675469404",
		"@tensorflow-models/face-detection": "^1.0.2",
		"@tensorflow/tfjs": "^4.10.0",
		"@tensorflow/tfjs-backend-wasm": "^4.10.0",
		"@webaudiomodules/api": "^2.0.0-alpha.6",
		"@webaudiomodules/sdk": "^0.0.11",
		"@webaudiomodules/sdk-parammgr": "^0.0.11",
		"abcjs": "^6.2.2",
		"analytics": "^0.8.9",
		"audioworklet-polyfill": "^1.1.2",
		"dialog-polyfill": "^0.5.6",
		"electron-is-dev": "^2.0.0",
		"holoplay-core": "^0.0.9",
		"midi-writer-js": "^3.0.1",
		"mpe": "^1.0.4",
		"share-menu": "^5.0.1",
		"stats.js": "^0.17.0",
		"three": "^0.156.1",
		"webmidi": "^3.1.6",
		"workbox-cacheable-response": "^7.0.0",
		"workbox-core": "^7.0.0",
		"workbox-expiration": "^7.0.0",
		"workbox-google-analytics": "^7.0.0",
		"workbox-precaching": "^7.0.0",
		"workbox-range-requests": "^7.0.0",
		"workbox-recipes": "^7.0.0",
		"workbox-routing": "^7.0.0",
		"workbox-strategies": "^7.0.0",
		"workbox-sw": "^7.0.0"
	},
	"devDependencies": {
		"@bubblewrap/cli": "^1.21.0",
		"@mediapipe/tasks-vision": "^0.10.4",
		"@parcel/optimizer-esbuild": "^2.9.3",
		"@parcel/packager-raw-url": "2.9.3",
		"@parcel/packager-xml": "2.9.3",
		"@parcel/transformer-inline-string": "2.9.3",
		"@parcel/transformer-pug": "2.9.3",
		"@parcel/transformer-sass": "2.9.3",
		"@parcel/transformer-webmanifest": "2.9.3",
		"@parcel/transformer-worklet": "2.9.3",
		"@parcel/transformer-xml": "2.9.3",
		"audio-decode": "^2.1.5",
		"bestbase64utf8": "^1.0.3",
		"electron": "^26.2.0",
		"electron-debug": "^3.2.0",
		"electron-icon-maker": "^0.0.5",
		"electron-packager": "^17.1.2",
		"electron-reloader": "^1.2.3",
		"generate-changelog": "^1.8.0",
		"gh-pages": "^6.0.0",
		"jest": "^29.6.4",
		"jsdoc": "^4.0.2",
		"jstransformer-markdown-it": "^3.0.0",
		"parcel": "^2.9.3",
		"parcel-namer-without-hash": "^0.0.1",
		"parcel-reporter-static-files-copy": "^1.5.2",
		"path-browserify": "^1.0.1",
		"postcss": "^8.4.29",
		"process": "^0.11.10",
		"sass": "^1.66.1",
		"showdown": "^2.1.0"
	},
	"packageManager": "yarn@3.4.1"
}