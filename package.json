{
  "name": "musical-face",
  "version": "0.0.2",
  "repository": "https://github.com/designerzen/MusicalFace.git",
  "author": "designerzen <designerzen@gmail.com>",
  "license": "GPL2.0",
  "scripts": {
    "clean": "node clean.js",
    "test": "parcel serve source/test.html",
    "start": "parcel serve source/index.html",
    "build": "parcel build --public-url ./ source/index.html",
    "builds": "parcel build --no-scope-hoist --public-url ./ source/index.html",
    "analyse": "parcel build source/index.html --profile --detailed-report",
    "deploy": "node deploy.js",
    "version": "parcel -V",
    "upgrader": "yarn upgrade-interactive",
    "package": "yarn clean && yarn build",
    "finalise": "yarn clean && yarn build && yarn deploy"
  },
  "dependencies": {
    "@capacitor/core": "^2.4.5",
    "@capacitor/ios": "^2.4.5",
    "@pwabuilder/pwainstall": "^1.6.7",
    "@pwabuilder/pwaupdate": "^0.2.0",
    "@tensorflow-models/face-landmarks-detection": "^0.0.2",
    "@tensorflow/tfjs": "^2.8.2",
    "@tensorflow/tfjs-backend-cpu": "^2.8.2",
    "@tensorflow/tfjs-backend-wasm": "^2.8.2",
    "@tensorflow/tfjs-backend-webgl": "^2.8.2",
    "@tensorflow/tfjs-converter": "^2.8.2",
    "@tensorflow/tfjs-core": "^2.8.2",
    "capacitor-dark-mode": "^1.0.5",
    "webmidi": "^3.0.0-alpha.2"
  },
  "devDependencies": {
    "@capacitor/cli": "^2.4.5",
    "gh-pages": "^3.1.0",
    "parcel": "^2.0.0-beta.1",
    "sass": "^1.30.0"
  }
}
