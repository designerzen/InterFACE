{
	"name": "interface.place",
	"description": "Instant Musical Superpowers! Augmented Reality, Motion Capture Controlled Machine Learning Powered Accessible Audio Synthesizer",
	"version": "0.12.0",
	"time": 1664820221930,
	"author": "designerzen <designerzen@gmail.com>",
	"license": "GPL-2.0-only",
	"repository": "https://github.com/designerzen/InterFACE.git",
	"bugs": "https://github.com/designerzen/InterFACE/issues",
	"homepage": "https://interface.place",
	"keywords": [
		"music",
		"accessibility",
		"machine learning",
		"ai",
		"xr",
		"ar"
	],
	"scripts": {
		"test": "jest",
		"push:": "git push origin --tags && git push origin",
		"freshstart": "yarn reset && yarn start",
		"start": "parcel serve source/*.pug -p 909",
		"killport": "npx kill-port 909",
		"hardreset": "node actions/reset.mjs --total && yarn install",
		"reset": "node actions/reset.mjs",
		"upgrader": "yarn upgrade-interactive",
		"changes": "node actions/changelog.mjs",
		"clean": "node actions/clean.mjs",
		"copy": "node actions/copy.mjs",
		"clear": "yarn clean && yarn copy",
		"deploy": "node actions/deploy.mjs",
		"doc": "jsdoc -d docs --configure .jsdoc.json source/interface.js",
		"build": "parcel build --no-source-maps --no-scope-hoist --no-content-hash --public-url ./ source/*.pug",
		"build:debug": "parcel build --no-minify --no-scope-hoist --public-url ./ source/*.pug",
		"build:index": "parcel build --no-scope-hoist --no-content-hash --public-url ./ source/index.pug",
		"run:electron": "electron . --enable-logging",
		"test:electron": "electron ./main.cjs --enable-logging",
		"build:electron": "electron .",
		"package:electron": "electron-packager . photosynth-interface --overwrite --out=releases --icon=./spirce/assets/icons/icon.ico",
		"open:electron": "npx cap open @capacitor-community/electron",
		"build:android": "parcel build --public-url ./ --dist-dir ./dist --no-source-maps source/app.pug",
		"open:android": "npx cap open android",
		"run:android": "npx cap run android",
		"android": "yarn clear && yarn build:android && node actions/app.mjs && yarn open:android",
		"help": "parcel build --public-url ./ source/help.pug",
		"roadmap": "parcel build --public-url ./ source/roadmap.pug",
		"app": "parcel build --public-url ./ --dist-dir ./app --no-source-maps source/app.pug",
		"app:build": "parcel build --public-url ./ --dist-dir ./app --no-source-maps source/app.pug",
		"copyrights:list": "NODE_ENV=production yarn licenses list && node license.mjs",
		"license": "yarn licenses generate-disclaimer > LICENSES.md && yarn licenses generate-disclaimer > source/licenses.md",
		"license:build": "parcel build --public-url ./ source/licenses.pug",
		"revision": "yarn version --patch --no-git-tag-version",
		"revise": "yarn revision && node actions/revision.mjs",
		"patch": "changelog -p && npm version patch --force && node actions/revision.mjs",
		"minor": "changelog -m && ynpm version minor --force && node actions/revision.mjs",
		"major": "changelog -M && npm version major --force && node actions/revision.mjs",
		"package": "yarn revise && yarn clear && yarn build",
		"analyse": "parcel build source/index.pug --profile --detailed-report",
		"publish:www": "yarn clear && yarn build && yarn deploy",
		"publish:cloudflare": "yarn clear && yarn build && yarn deploy",
		"release:patch": "npm version patch --force && yarn publish:www",
		"release:minor": "npm version minor --force && yarn publish:www",
		"release:major": "npm version major --force && yarn publish:www",
		"release:app": "yarn patch && yarn push",
		"status": "electron-packager --version"
	},
	"dependencies": {
		"@analytics/google-analytics": "^1.0.5",
		"@capacitor/electron": "^2.5.0",
		"@lookingglass/webxr": "^0.3.1",
		"@mediapipe/face_detection": "^0.4.1646425229",
		"@mediapipe/face_mesh": "^0.4.1633559619",
		"@mediapipe/pose": "^0.5.1675469404",
		"@tensorflow-models/face-detection": "^1.0.1",
		"@tensorflow-models/face-landmarks-detection": "^1.0.2",
		"@tensorflow-models/pose-detection": "^2.1.0",
		"@tensorflow/tfjs": "^4.2.0",
		"@tensorflow/tfjs-backend-cpu": "^4.2.0",
		"@tensorflow/tfjs-backend-wasm": "^4.2.0",
		"@tensorflow/tfjs-backend-webgl": "^4.2.0",
		"@tensorflow/tfjs-converter": "^4.2.0",
		"@tensorflow/tfjs-core": "^4.2.0",
		"@tonaljs/tonal": "^4.10.0",
		"@webaudiomodules/api": "^2.0.0-alpha.6",
		"@webaudiomodules/sdk": "^0.0.11",
		"@webaudiomodules/sdk-parammgr": "^0.0.11",
		"analytics": "^0.8.1",
		"audioworklet-polyfill": "^1.1.2",
		"dialog-polyfill": "^0.5.6",
		"electron-is-dev": "^2.0.0",
		"holoplay-core": "^0.0.9",
		"midi-writer-js": "^2.1.4",
		"mpe": "^1.0.4",
		"share-menu": "^5.0.0-rc.4",
		"stats.js": "^0.17.0",
		"three": "^0.150.1",
		"webmidi": "^3.1.5",
		"workbox-cacheable-response": "^6.5.4",
		"workbox-core": "^6.5.4",
		"workbox-expiration": "^6.5.4",
		"workbox-google-analytics": "^6.5.4",
		"workbox-precaching": "^6.5.4",
		"workbox-range-requests": "^6.5.4",
		"workbox-recipes": "^6.5.4",
		"workbox-routing": "^6.5.4",
		"workbox-strategies": "^6.5.4",
		"workbox-sw": "^6.5.4"
	},
	"devDependencies": {
		"@bubblewrap/cli": "^1.19.1",
		"@parcel/packager-raw-url": "2.8.3",
		"@parcel/packager-xml": "2.8.3",
		"@parcel/transformer-pug": "2.8.3",
		"@parcel/transformer-sass": "2.8.3",
		"@parcel/transformer-webmanifest": "2.8.3",
		"@parcel/transformer-xml": "2.8.3",
		"electron": "^23.2.0",
		"electron-debug": "^3.2.0",
		"electron-icon-maker": "^0.0.5",
		"electron-packager": "^17.1.1",
		"electron-reloader": "^1.2.3",
		"generate-changelog": "^1.8.0",
		"gh-pages": "^5.0.0",
		"jest": "^29.5.0",
		"jsdoc": "^4.0.2",
		"jstransformer-markdown-it": "^3.0.0",
		"parcel": "^2.8.3",
		"parcel-reporter-static-files-copy": "^1.5.0",
		"postcss": "^8.4.21",
		"process": "^0.11.10",
		"sass": "^1.60.0",
		"showdown": "^2.1.0"
	},
	"packageManager": "yarn@3.4.1"
}
