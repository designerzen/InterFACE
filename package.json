{
	"name": "musical-face",
	"version": "0.42.0",
	"repository": "https://github.com/designerzen/InterFACE.git",
	"author": "designerzen <designerzen@gmail.com>",
	"license": "GPL-2.0-only",
	"type": "module",
	"scripts": {
		"start": "parcel serve source/index.pug -p 909",
		"build": "parcel build --no-scope-hoist --public-url ./ source/index.pug",
		"build:debug": "parcel build --no-minify --no-scope-hoist --public-url ./ source/index.pug",
		"app": "parcel build --public-url ./ --dist-dir ./app --no-source-maps source/app.pug",
		"hardreset": "node actions/reset.js --total && yarn install",
		"reset": "yarn killport && node actions/reset.js",
		"killport": "npx kill-port 909",
		"clean": "node actions/clean.js",
		"copyrights:list": "NODE_ENV=production yarn licenses list && node license.js",
		"license": "yarn licenses generate-disclaimer > LICENSES.md",
		"revision": "yarn version --patch --no-git-tag-version",
		"revise": "yarn revision && node actions/revision.js",
		"patch": "yarn version --patch && node actions/revision.js",
		"minor": "yarn version --minor && node actions/revision.js",
		"major": "yarn version --major && node actions/revision.js",
		"package": "yarn revise && yarn clean && yarn build",
		"analyse": "parcel build source/index.pug --profile --detailed-report",
		"deploy": "node actions/deploy.js",
		"upgrader": "yarn upgrade-interactive",
		"hotfix": "yarn patch && yarn clean && yarn build && yarn deploy",
		"release": "yarn minor && yarn clean && yarn build && yarn deploy"
	},
	"dependencies": {
		"@analytics/google-analytics": "^0.5.2",
		"@capacitor/core": "^2.4.5",
		"@capacitor/ios": "^2.4.5",
		"@tensorflow-models/face-landmarks-detection": "^0.0.2",
		"@tensorflow/tfjs": "^2.8.2",
		"@tensorflow/tfjs-backend-cpu": "^2.8.2",
		"@tensorflow/tfjs-backend-wasm": "^2.8.2",
		"@tensorflow/tfjs-backend-webgl": "^2.8.2",
		"@tensorflow/tfjs-converter": "^2.8.2",
		"@tensorflow/tfjs-core": "^2.8.2",
		"analytics": "^0.6.6",
		"capacitor-dark-mode": "^1.0.5",
		"mpe": "^1.0.4",
		"share-menu": "^4.0.0",
		"webmidi": "^3.0.0-alpha.2"
	},
	"devDependencies": {
		"@capacitor/cli": "^2.4.5",
		"@parcel/optimizer-data-url": "2.0.0-beta.1",
		"@parcel/packager-raw-url": "2.0.0-beta.1",
		"@parcel/transformer-inline-string": "2.0.0-beta.1",
		"@parcel/transformer-pug": "2.0.0-beta.1",
		"@parcel/transformer-sass": "2.0.0-beta.1",
		"@parcel/transformer-webmanifest": "^2.0.0-beta.1",
		"gh-pages": "^3.1.0",
		"parcel": "2.0.0-beta.1",
		"postcss": "^8.2.1",
		"pug": "^3.0.0",
		"sass": "^1.30.0"
	}
}
